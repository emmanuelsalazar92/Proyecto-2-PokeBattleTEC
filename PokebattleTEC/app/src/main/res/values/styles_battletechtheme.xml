	  <item name="android:textColor">#000000</item>
  </style>

  <style name="CheckBoxBattleTechTheme" parent="android:Widget.CompoundButton.CheckBox">
      <item name="android:button">@drawable/battletechtheme_btn_check_holo_light</item>
  </style>

  <style name="RadioButtonBattleTechTheme" parent="android:Widget.CompoundButton.RadioButton">
      <item name="android:button">@drawable/battletechtheme_btn_radio_holo_light</item>
  </style>

  <style name="ButtonBattleTechTheme" parent="android:Widget.Button">
	  <item name="android:background">@drawable/battletechtheme_btn_default_holo_light</item>
	  <item name="android:minHeight">48dip</item>
	  <item name="android:minWidth">64dip</item>
	  <item name="android:textColor">#000000</item>
  </style>

  <style name="ImageButtonBattleTechTheme" parent="android:Widget.ImageButton">
	  <item name="android:background">@drawable/battletechtheme_btn_default_holo_light</item>
  </style>

  <style name="SpinnerBattleTechTheme" parent="android:Widget.Spinner">
      <item name="android:background">@drawable/battletechtheme_spinner_background_holo_light</item>
      <item name="android:dropDownSelector">@drawable/battletechtheme_list_selector_holo_light</item>
  </style>

  <style name="SpinnerDropDownItemBattleTechTheme" parent="android:Widget.DropDownItem.Spinner">
      <item name="android:checkMark">@drawable/battletechtheme_btn_radio_holo_light</item>
  </style>
  <style name="TabBattleTechTheme">
      <item name="android:gravity">center_horizontal</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
      <item name="android:background">@drawable/battletechtheme_tab_indicator_holo</item>
      <item name="android:layout_width">0dip</item>
      <item name="android:layout_weight">1</item>
      <item name="android:minWidth">80dip</item>
  </style>

  <style name="TabTextBattleTechTheme">
	  <item name="android:textColor">#000000</item>
      <item name="android:textSize">12sp</item>
      <item name="android:textStyle">bold</item>
      <!-- v14 <item name="android:textAllCaps">true</item> -->
      <item name="android:ellipsize">marquee</item>
      <item name="android:maxLines">2</item>
      <item name="android:maxWidth">180dip</item>
  </style>

  <style name="ProgressBarBattleTechTheme" parent="android:Widget.ProgressBar.Horizontal">
      <item name="android:progressDrawable">@drawable/battletechtheme_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/battletechtheme_progress_indeterminate_horizontal_holo_light</item>
      <item name="android:minHeight">16dip</item>
      <item name="android:maxHeight">16dip</item>
  </style>

  <style name="SeekBarBattleTechTheme" parent="android:Widget.SeekBar">
      <item name="android:progressDrawable">@drawable/battletechtheme_scrubber_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/battletechtheme_scrubber_progress_horizontal_holo_light</item>
      <item name="android:minHeight">13dip</item>
      <item name="android:maxHeight">13dip</item>
      <item name="android:thumb">@drawable/battletechtheme_scrubber_control_selector_holo_light</item>
      <item name="android:thumbOffset">16dip</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
  </style>

  <style name="RatingBarBattleTechTheme" parent="android:Widget.RatingBar">
      <item name="android:progressDrawable">@drawable/battletechtheme_ratingbar_full_holo_light</item>
